{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "RWObjects",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject"
            ],
            "Resource": [
                "arn:aws:s3:::bucket-name/abc",
                "arn:aws:s3:::bucket-name/abc/*"
            ]
        }
    ]
}